		int count = 0;
		Scanner scan = new Scanner(System.in);
		String str = new String();
		int m = scan.nextInt();
		str = scan.next();
		int n = str.length();
		while( n-count < m)
		{
			
		}
		for(int i = 0; n - count < m ; i++)
		{
			if(n-i-1<m)
			{
				i = m - n + i;
			}
		}
	}
}
