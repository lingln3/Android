import java.util.Scanner;


public class Fourteen {
	String s;
	char[] ch;
	public void change()
	{
		Scanner scan = new Scanner(System.in);
		s = scan.next();
		ch = s.toCharArray();
		int size = s.length();
		for(int i = 0; i < size-1 ; i++)
		{
			if(ch[i] == ch[i+1])
			{
				ch[i] = 
				i = i +2;
			}
		}
	}
	public static char isZ(char c)
	{
		char result = '0';
		if(c == 'z')
		{
			result = 'a';
		}
		else{
			result = c + 1;
		}
		return result;
	}
}
