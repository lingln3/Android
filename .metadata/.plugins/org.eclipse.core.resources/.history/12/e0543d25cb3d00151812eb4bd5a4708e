import java.util.Scanner;


public class Main {
	public static void main(String[] args)
	{
		Scanner scan = new Scanner(System.in);
		int n = 0;
		n = scan.nextInt();
		System.out.println(shizi(n));
		System.out.println(n/2*2+Math.pow(-1, n-1));
	}
	public static int shizi(int n)
	{
		int biaozhi = 0;
		int fi[];
		int count = 0;
		for(int i = 0 ; ; i++)
		{
			if(n/10 != 0) count++;
			else count--;
		}
	}	
}
