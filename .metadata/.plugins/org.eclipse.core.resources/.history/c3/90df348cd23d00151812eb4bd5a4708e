import java.util.Scanner;


public class Main {

	char[] pcAddend;
	char[] pcAugend;
	char[] pcAddResult;
	
	public static void main(String[] args)
	{		
		String str1,str2;
		Main main = new Main();		
		Scanner scan = new Scanner(System.in);
		str1 = scan.next();
		str2 = scan.next();
		main.pcAddend = new char[str1.length()];
		main.pcAugend = new char[str2.length()];
		main.pcAddend = str1.toCharArray();
		main.pcAugend = str2.toCharArray();
		
	}
	public void AddLongInteger(Main main)
	{
		int jinwei = 0;
		int length = Math.max(main.pcAddend.length, main.pcAugend.length);
		
		
	}
}
