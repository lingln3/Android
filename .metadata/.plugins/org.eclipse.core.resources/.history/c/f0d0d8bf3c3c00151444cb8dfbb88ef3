import java.util.Scanner;


public class Five {
	public void function()
	{		
		Scanner scan = new Scanner(System.in);
		String str[] = new String[3];
		System.out.println("please input the function");
		for(int i = 0 ; i < str.length; i++)
		{
			str[i] = scan.next();
		}
		for(String st:str)
		{
			System.out.println(str);
		}
		
	}
	
}
